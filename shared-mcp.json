{
  "mcpServers": {
    "dev-tools": {
      "command": "npx",
      "instruction": "Use this to launch browser and interact with it",
      "args": ["-y", "chrome-devtools-mcp@latest"],
      "env": {}
    },
    "scaffold-mcp": {
      "command": "npx",
      "args": ["-y", "@agiflowai/scaffold-mcp", "mcp-serve"],
      "env": {}
    },
    "architect-mcp": {
      "command": "npx",
      "args": ["-y", "@agiflowai/architect-mcp", "mcp-serve"],
      "env": {}
    }
  }
}
