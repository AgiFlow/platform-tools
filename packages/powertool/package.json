{
  "name": "@agiflowai/powertool",
  "description": "AgiFlow MCP toolkit server with tools and utilities",
  "version": "0.1.0",
  "license": "SEE LICENSE IN LICENSE.md",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "typescript"
  ],
  "bin": {
    "@agiflowai/powertool": "./dist/cli.cjs"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "module": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "node --loader ts-node/esm src/cli.ts mcp-serve",
    "build": "tsdown",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.9.0",
    "@modelcontextprotocol/sdk": "1.20.2",
    "chalk": "5.6.2",
    "commander": "14.0.1",
    "liquidjs": "10.21.1",
    "express": "^4.21.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "tsdown": "^0.15.6",
    "unplugin-raw": "^0.6.3",
    "typescript": "5.9.3",
    "vitest": "^2.1.8"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs"
    },
    "./package.json": "./package.json"
  }
}
